{
  "name": "@mov-ai/mov-fe-lib-core",
  "version": "1.2.1-7",
  "description": "The Mov.AI's core frontend library.",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/mov-ai"
  },
  "main": "cjs/mov-core.js",
  "browser": "web/mov-core.js",
  "module": "esm/mov-core.js",
  "entry": "src/mov-core.ts",
  "types": "types/mov-core.d.ts",
  "library": "Movai",
  "external": {
    "lodash": "_"
  },
  "scripts": {
    "lock": "mv ../../pnpm-workspace.yaml ../../pnpm-workspace.yaml.bak ; pnpm i ; mv ../../pnpm-workspace.yaml.bak ../../pnpm-workspace.yaml",
    "test": "scripts test",
    "testDev": "jest --watchAll",
    "testStatic": "webpack --mode=development && echo 'Open browser at http://localhost:3030/test/index.html' && node test/server.js",
    "build": "scripts build",
    "watch": "scripts watch",
    "clean": "rm -rf cjs esm web types"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MOV-AI/frontend-npm-lib-core.git"
  },
  "keywords": [
    "frontend",
    "core",
    "common",
    "utils"
  ],
  "author": "MOV.AI",
  "license": "ISC",
  "homepage": "https://github.com/MOV-AI/frontend-npm-lib-core/blob/main/README.md",
  "devDependencies": {
    "@tty-pt/scripts": "^0.6.0-27",
    "@types/jest": "27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.55",
    "@types/roslib": "^1.1.8",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6"
  },
  "dependencies": {
    "fast-equals": "^5.0.1",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "roslib": "^1.4.1",
    "rxjs": "^7.3.0"
  },
  "files": [
    "cjs",
    "web",
    "esm",
    "types",
    "resources"
  ],
  "typesVersions": {
    "*": {
      "api": [
        "dist/src/api/index.d.ts"
      ],
      "models": [
        "dist/src/models/index.d.ts"
      ]
    }
  }
}
