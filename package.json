{
  "name": "@mov-ai/mov-fe-lib-core",
  "version": "1.1.2-20",
  "description": "The Mov.AI's core frontend library.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "decl": "tsc",
    "test": "jest",
    "testDev": "jest --watchAll",
    "build": "tsc && swc --copy-files -d dist src",
    "clean": "rm -r node_modules; rm package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MOV-AI/frontend-npm-lib-core.git"
  },
  "keywords": [
    "frontend",
    "core",
    "common",
    "utils"
  ],
  "author": "MOV.AI",
  "license": "ISC",
  "homepage": "https://github.com/MOV-AI/frontend-npm-lib-core/blob/main/README.md",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@types/jest": "27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/roslib": "^1.1.8",
    "@types/web": "^0.0.114",
    "jest": "^28.1.0"
  },
  "dependencies": {
    "deep-equal": "^2.2.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "monet": "^0.9.2",
    "roslib": "^1.3.0",
    "rxjs": "^7.3.0",
    "typescript": "^4.7.2"
  },
  "files": [
    "dist",
    "resources"
  ],
  "typesVersions": {
    "*": {
      "api": [
        "dist/src/api/index.d.ts"
      ],
      "models": [
        "dist/src/models/index.d.ts"
      ]
    }
  }
}
